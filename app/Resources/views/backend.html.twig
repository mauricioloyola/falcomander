{% extends '::base.html.twig' %}

{% block stylesheets %}   
  <link href="http://fonts.googleapis.com/css?family=Open+Sans:300italic,400italic,600italic,700italic,800italic,400,300,600,700,800" rel="stylesheet">

  <link href="{{ asset(global.directorio_css ~ "bootstrap.min.css")}}" rel="stylesheet"/>
  <link href="{{ asset(global.directorio_css ~ "normalize.min.css")}}" rel="stylesheet"/>
  <link href="{{ asset(global.directorio_css ~ "font-awesome.min.css")}}" rel="stylesheet"/>
  <link href="{{ asset(global.directorio_css ~ "animate.css")}}" rel="stylesheet"/>
  <link href="{{ asset(global.directorio_css ~ "templatemo_misc.css")}}" rel="stylesheet"/>
  <link href="{{ asset(global.directorio_css ~ "templatemo_style.css")}}" rel="stylesheet"/>
  <link href="{{ asset(global.directorio_css ~ "templatemo_style.css")}}" rel="stylesheet"/>
  <script src="{{ asset(global.directorio_js ~ "vendor/modernizr-2.6.2.min.js")}}"></script>
{% endblock %}

{% block metadesc %}{{"Control de Plaga Aviar, Cetrería, Etología"}}{% endblock %}

{% block metaetiq %}{{"cetrería, cetreria, etología, etologia, aviar, aves, control de plaga aviar, control de plaga de aves, control de plaga palomas, plaga palomas, plaga aves, plaga gallinazos, plaga de aves en industria, palomas en almacen, almacen, aves en almacenes, gavilan, halcon, palomas en empresas, empresas, oficinas, centro comerciales, erradicar palomas, controlar palomas, controlar, erradicar, biologico, control biologico"}}{% endblock %}

{% block body %}

  <div id="front">
        <div class="site-header">
            <div class="container">
                <div class="row">
                    <div class="col-md-4 col-sm-6 col-xs-6">
                        <div id="templatemo_logo">
                            <h1><a href="#" title="HTML5 Template">{{"Control de Plaga Aviar"}}</a></h1>
                        </div> <!-- /.logo -->
                    </div> <!-- /.col-md-4 -->
                    <div class="col-md-8 col-sm-6 col-xs-6">
                        <a href="#" class="toggle-menu"><i class="fa fa-bars"></i></a>
                        <div class="main-menu">
                            <ul>
                                <li><a href="#front">{{"Inicio"}}</a></li>
                                <li><a href="#empresa">{{"Empresa"}}</a></li>
                                <li><a href="#servicios">{{"Servicios"}}</a></li>
                                <li><a href="#contact">{{"Contacto"}}</a></li>
                            </ul>
                        </div> <!-- /.main-menu -->
                    </div> <!-- /.col-md-8 -->
                </div> <!-- /.row -->
                <div class="row">
                    <div class="col-md-12">
                        <div class="responsive">
                            <div class="main-menu">
                                <ul>
                                    <li><a href="#front">{{"Inicio"}}</a></li>
                                    <li><a href="#empresa">{{"Empresa"}}</a></li>
                                    <li><a href="#products">{{"Servicios"}}</a></li>
                                    <li><a href="#contact">{{"Contacto"}}</a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div> <!-- /.container -->
        </div> <!-- /.site-header -->
  </div> <!-- /#front -->

  <div class="site-slider">
    {% block slider %}{% endblock %}        
  </div> <!-- /.site-slider -->  

  <div id="empresa" class="content-section">
    <div class="container">
      {% block empresa %}{% endblock %}        
    </div> <!-- /.container -->
  </div> <!-- /#product-promotion -->      

  <div id="palomas" class="content-section">
    <div class="container">
      {% block palomas %}{% endblock %}        
    </div> <!-- /.container -->
  </div> <!-- /#product-promotion --> 

  <div id="servicios" class="content-section">
    <div class="container">
      {% block servicios %}{% endblock %}        
    </div> <!-- /.container -->
  </div> <!-- /#product-promotion -->  

{% endblock %}        

{% block javascripts %}  
  <script src="{{ asset(global.directorio_js ~ "vendor/jquery-1.10.1.min.js")}}"></script>
  <script src="{{ asset(global.directorio_js ~ "vendor/jquery-1.10.1.min.js")}}"></script>
  <script>window.jQuery || document.write('<script src="{{ asset(global.directorio_js ~ "vendor/jquery-1.10.1.min.js")}}"><\/script>')</script>
  <script src="{{ asset(global.directorio_js ~ "jquery.easing-1.3.js")}}"></script>
  
  <script src="{{ asset(global.directorio_js ~ "bootstrap.js")}}"></script>
  <script src="{{ asset(global.directorio_js ~ "plugins.js")}}"></script>
  <script src="{{ asset(global.directorio_js ~ "main.js")}}"></script> 
{% endblock %}

